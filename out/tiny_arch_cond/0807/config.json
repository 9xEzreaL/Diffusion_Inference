{
    "name": "test_pain",
    "gpu_ids": [
        0
    ],
    "seed": -1,
    "finetune_norm": false,
    "path": {
        "base_dir": "out",
        "results": "results",
        "resume_state": "submodels/tiny_arch_cond/200",
        "exp_name": "tiny_arch_cond/0807"
    },
    "datasets": {
        "test": {
            "which_dataset": {
                "name": "PainValidationDataset",
                "args": {
                    "data_root": "/media/ziyi/Dataset/OAI_pain/full/ap/*",
                    "mask_config": {
                        "mask_mode": "hybrid"
                    },
                    "ids": [
                        1223,
                        1245,
                        2698,
                        5591,
                        6909,
                        9255,
                        9351,
                        9528
                    ]
                }
            },
            "dataloader": {
                "args": {
                    "batch_size": 8,
                    "num_workers": 4,
                    "pin_memory": true
                }
            }
        }
    },
    "model": {
        "which_model": {
            "name": [
                "models.model",
                "Palette"
            ],
            "args": {
                "sample_num": 8,
                "task": "inpainting",
                "ema_scheduler": {
                    "ema_start": 1,
                    "ema_iter": 1,
                    "ema_decay": 0.9999
                },
                "optimizers": [
                    {
                        "lr": 5e-05,
                        "weight_decay": 0
                    }
                ]
            }
        },
        "which_networks": [
            {
                "name": [
                    "models.network",
                    "Network"
                ],
                "args": {
                    "init_type": "kaiming",
                    "module_name": "guided_diffusion",
                    "unet": {
                        "in_channel": 2,
                        "out_channel": 1,
                        "inner_channel": 32,
                        "channel_mults": [
                            1,
                            1,
                            2,
                            4
                        ],
                        "attn_res": [
                            16
                        ],
                        "num_head_channels": 16,
                        "res_blocks": 1,
                        "dropout": 0.2,
                        "image_size": 256
                    },
                    "beta_schedule": {
                        "train": {
                            "schedule": "linear",
                            "n_timestep": 2000,
                            "linear_start": 1e-06,
                            "linear_end": 0.01
                        },
                        "test": {
                            "schedule": "linear",
                            "n_timestep": 2000,
                            "linear_start": 1e-06,
                            "linear_end": 0.01
                        }
                    }
                }
            }
        ],
        "which_losses": [
            "mse_loss"
        ],
        "which_metrics": [
            "mae"
        ]
    },
    "train": {
        "n_epoch": 100000000.0,
        "n_iter": 100000000.0,
        "val_epoch": 10,
        "save_checkpoint_epoch": 10,
        "log_iter": 1000.0,
        "tensorboard": true
    },
    "debug": {
        "val_epoch": 1,
        "save_checkpoint_epoch": 1,
        "log_iter": 2,
        "debug_split": 50
    },
    "phase": "test",
    "distributed": false
}